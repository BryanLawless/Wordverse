{
	"env": {
		"node": true
	},
	"extends": ["plugin:security/recommended", "plugin:prettier/recommended"],
	"plugins": ["security", "prettier"],
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"rules": {
		"no-console": "error",
		"func-names": "off",
		"no-underscore-dangle": "off",
		"consistent-return": "off",
		"security/detect-object-injection": "off"
	}
}
